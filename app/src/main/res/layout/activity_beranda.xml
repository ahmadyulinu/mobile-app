<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Beranda">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/backgrounddasar"
        android:contentDescription="@string/backgroundbiru" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:srcCompat="@drawable/backgroundatas"
        android:contentDescription="@string/backgroundhijau" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="-79dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="99dp"
            android:layout_marginStart="30dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/Beranda"
                android:layout_width="match_parent"
                android:layout_height="69dp"
                android:layout_marginTop="20dp"
                android:text="@string/beranda"
                android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                android:textColor="#000000"
                android:textSize="30sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="150dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/katikan1"
                android:contentDescription="@string/kategoriikan1" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="340dp"
            android:layout_height="58dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/ikan_pilihan"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="24sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="340dp"
            android:layout_height="100dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:background="@drawable/kolomkategori">

            <LinearLayout
                android:layout_width="131dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/ikan1"
                    android:contentDescription="@string/ikan1" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:fontFamily="@font/cardo_bold"
                    android:text="@string/namaikan1"
                    android:textColor="#000000"
                    android:textSize="18sp" />

                <EditText
                    android:id="@+id/editTextTextPersonName3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:autofillHints=""
                    android:ems="10"
                    android:fontFamily="@font/montserrat"
                    android:inputType="textPersonName"
                    android:labelFor="@id/textView"
                    android:text="@string/harga1"
                    android:textColor="#000000"
                    android:textSize="12sp"
                    android:contentDescription="@string/harga1"
                    tools:ignore="LabelFor" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="340dp"
            android:layout_height="100dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/kolomkategori"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="131dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/ikan2"
                    android:contentDescription="@string/ikan2" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:text="@string/nama_ikan2"
                    android:fontFamily="@font/cardo_bold"
                    android:textColor="#000000"
                    android:textSize="18sp"/>

                <EditText
                    android:id="@+id/editTextTextPersonName4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:ems="10"
                    android:fontFamily="@font/montserrat"
                    android:inputType="textPersonName"
                    android:text="@string/harga2"
                    android:textColor="#000000"
                    android:textSize="12sp"
                    android:autofillHints=""
                    tools:ignore="LabelFor" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="340dp"
            android:layout_height="100dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/kolomkategori"
            android:orientation="horizontal">


            <LinearLayout
                android:layout_width="131dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/ikan3"
                    android:contentDescription="@string/ikan3" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:fontFamily="@font/cardo_bold"
                    android:text="@string/namaikan3"
                    android:textColor="#000000"
                    android:textSize="18sp" />

                <EditText
                    android:id="@+id/editTextTextPersonName5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:autofillHints="hint"
                    android:ems="10"
                    android:fontFamily="@font/montserrat"
                    android:inputType="textPersonName"
                    android:labelFor="@id/textView4"
                    android:text="@string/harga3"
                    android:textColor="#000000"
                    android:textSize="12sp"
                    tools:ignore="LabelFor" />
            </LinearLayout>


        </LinearLayout>



    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>